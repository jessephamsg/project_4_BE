const mongoose = require('mongoose');
const Schema = mongoose.Schema; 

const KidSchema = new Schema({
    name: {
      type: String,
      required: true
    },
    email: {
      type: String,
      required: true
    },
    password: {
      type: String,
    },
    kids_id: [{
        kid_id: String
    }],
    timestamps: true
  });

const Kid = mongoose.model('Kid', KidSchema)
module.exports = Kid;